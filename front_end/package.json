{
  "name": "prototype",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.1.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "antd": "^4.2.4",
    "history": "^5.0.0",
    "node-sass": "^4.14.1",
    "prop-types": "^15.7.2",
    "quill": "^1.3.7",
    "react": "^16.13.1",
    "react-codemirror": "^1.0.0",
    "react-dom": "^16.13.1",
    "react-markdown": "^4.3.1",
    "react-quilljs": "^1.2.11",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "unfetch": "^4.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "clear_docs": "rm -rf ../docs/*",
    "clear_static": "rm -rf ../back_end/src/main/resources/static/*",
    "move_to_docs": "mv ./build/* ../docs/",
    "copy_to_docs": "cp -r ./build/* ../docs/",
    "move_to_rear": "mv ./build/* ../back_end/src/main/resources/static/",
    "build_D": "npm run build && npm run clear_docs && npm run move_to_docs",
    "build_R": "npm run build && npm run clear_static && npm run move_to_rear",
    "build_A": "npm run build && npm run clear_docs && npm run clear_static && npm run copy_to_docs && npm run move_to_rear",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:8080"
}
